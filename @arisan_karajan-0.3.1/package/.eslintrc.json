{
  "env": {
    "mocha": true,
    "node": true
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "no-console": 0,
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "spaced-comment": [
      "error",
      "always",
      {
        "exceptions": [
          "endregion"
        ],
        "markers": [
          "noinspection",
          "region"
        ]
      }
    ]
  }
}
